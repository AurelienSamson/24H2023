{% extends 'base.html.twig' %}

{% block title %}Circuits Sop'Racing{% endblock %}

{% block body %}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.bundle.min.js" integrity="sha384-qKXV1j0HvMUeCBQ+QVp7JcfGl760yU08IQ+GpUo5hlbpg51QRiuqHAJz8+BrxE/N" crossorigin="anonymous"></script>

{% include('navbar.html.twig') %}

<div class="row">


{% for item in images_circuit_offi %}

 <div class="col-sm-3">
  <div class="card">
   <img src="/assets/images/races/officielles/{{ item.image }}" class="card-img-top" alt="...">
   <div class="card-body">
     <p class="card-text">{{ item.name }}</p>
   </div>
  </div>
  </div>


    
{% endfor %}

</div>

{% for item in images_circuit_option %}
<div class="row">
 <div class="col-sm-4">
<div class="card" >
  <img src="/assets/images/races/optionnelles/{{ item }}" class="card-img-top" alt="...">
  <div class="card-body">
    <p class="card-text">{{ item }}</p>
  </div>
  </div>
  </div>
</div>

    
{% endfor %}




{% endblock %}
